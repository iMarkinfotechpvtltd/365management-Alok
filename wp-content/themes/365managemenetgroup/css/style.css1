.frm-btns:hover{ background-color: transparent;  color:#d3a953; transition: all 0.3s ease; -moz-transition: all 0.3s ease; -webkit-transition: all 0.3s ease; -ms-transition: all 0.3s ease; -o-transition: all 0.3s ease;}
.set-map{height: 366px; border:1px solid #e8e8e8 !important; padding: 5px !important; background-color: #fff;}

