/*============================================== Contact Page ==============================================*/
.contact-address li {
    color: #4d4d4d;
    font-size: 16px;
    margin: 0 0 47px;
    text-align: left;
}
.contact-address li a{
    display: table;
    vertical-align: middle;
    color:#4d4d4d;
    font-size: 16px;
    transition: all 0.3s ease; -moz-transition: all 0.3s ease; -webkit-transition: all 0.3s ease; -ms-transition: all 0.3s ease; -o-transition: all 0.3s ease;
}
.contact-address li span {
    border: 1px solid #d3a953;
    border-radius: 50%;
    display: table-cell;
    font-size: 25px;
    height: 90px;
    color: #d3a953;
    text-align: center;
    vertical-align: middle;
    width: 90px;
    transition: all 0.3s ease; -moz-transition: all 0.3s ease; -webkit-transition: all 0.3s ease; -ms-transition: all 0.3s ease; -o-transition: all 0.3s ease;
}
.contact-address li a em {
    font-style: normal;
    line-height: 40px;
    margin-left: 30px;
}
.contact-address li em {
    display: table-cell;
    font-size: 16px;
    font-style: normal;
    padding-left: 30px;
    vertical-align: middle;
}
.contact-address li:first-child em {
    max-width: 240px;
    line-height: 40px;
}

.contact-address li a:hover span {
    border-color: #4d4d4d;
    color: #4d4d4d;
}
.contact-address li a:hover em{color:#d3a953;}


.form-section { background-color: #f2f2f2; padding: 65px 0;}
